{
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "platform": "github",
  "repositories": [
    "SabrinaPloner/renovate-vulnerability-playground"
  ],
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "groupName": "all dependencies",
      "groupSlug": "all",
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "major"
      ]
    }
  ],
  "recreateWhen": "always"
}