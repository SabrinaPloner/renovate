{
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "platform": "github",
  "repositories": [
    "SabrinaPloner/renovate-vulnerability-playground"
  ],
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "groupName": "all-mavem",
      "groupSlug": "all-maven",
      "enabled": "false",
      "matchManager": "maven"
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": false
  },
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "(^|/|\\.)custom-ds-example\\.xml$"
      ],
      "datasourceTemplate": "custom.grype",
      "matchStrings": [
        "<version>(?<versioning>.*?)</version>"
      ],
      "versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}"
    }
  ],
  "customDatasources": {
    "grype": {
      "defaultRegistryUrlTemplate": "file:///tmp/dependencies/{{packageName}}/versiontracker.json",
      "transformTemplates": [
        "{\n  \"releases\": [\n    {\n      \"version\": \"2.0\"\n    }]\n}"
      ]
    }
  },
  "recreateWhen": "always"
}
