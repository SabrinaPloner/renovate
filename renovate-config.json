{
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "platform": "github",
  "repositories": [
    "SabrinaPloner/renovate-vulnerability-playground"
  ],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 20,
  "packageRules": [
    {
      "enabled": false,
      "matchPackagePatterns": ["*"],
      "matchUpdateTypes": ["patch","minor", "pin", "pinDigest", "digest", "lockFileMaintenance", "rollback", "bump", "replacement"]
    }
  ],
"recreateWhen": "always"
}
