{
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "platform": "github",
  "repositories": [
    "SabrinaPloner/renovate-vulnerability-playground"
  ],
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "groupName": "security updates",
      "groupSlug": "security",
      "matchPackagePatterns:": [
        "*"
      ],
      "matchDatasources": ["custom.grype"],
      "enabled": "true"
    }
  ],
  "maven": {
    "enabled": false
  },
  "customDatasources": {
    "grype": {
      "defaultRegistryUrlTemplate": "file://packages/{{packageName}}"
    }
  },
  "recreateWhen": "always"
}